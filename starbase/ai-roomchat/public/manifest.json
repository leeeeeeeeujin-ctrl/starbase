  "categories": ["games", "developer", "productivity", "education"],
  "iarc_rating_id": "",
  
  "prefer_related_applications": false,
  "related_applications": [],
  
  "shortcuts": [
    {
      "name": "New Game Project",
      "short_name": "New Project",
      "description": "Create a new AI game project",
      "url": "/projects/new",
      "icons": [
        {
          "src": "/icons/shortcut-new.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "AI Code Assistant",
      "short_name": "AI Assistant",
      "description": "Open AI coding assistant",
      "url": "/ai-assistant",
      "icons": [
        {
          "src": "/icons/shortcut-ai.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Projects",
      "short_name": "Projects",
      "description": "View all my game projects",
      "url": "/projects",
      "icons": [
        {
          "src": "/icons/shortcut-projects.png",
          "sizes": "96x96", 
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Game Demo",
      "short_name": "Demo",
      "description": "Test game engine features",
      "url": "/demo",
      "icons": [
        {
          "src": "/icons/shortcut-demo.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  
  "file_handlers": [
    {
      "action": "/editor",
      "accept": {
        "application/javascript": [".js"],
        "application/json": [".json"],
        "text/python": [".py"],
        "text/sql": [".sql"],
        "text/markdown": [".md"]
      }
    }
  ],
  
  "protocol_handlers": [
    {
      "protocol": "starbase",
      "url": "/open?url=%s"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["text/*", "application/javascript", "application/json"]
        }
      ]
    }
  },
  
  "display_override": ["window-controls-overlay", "minimal-ui"],
  "edge_side_panel": {
    "preferred_width": 400
  }
}